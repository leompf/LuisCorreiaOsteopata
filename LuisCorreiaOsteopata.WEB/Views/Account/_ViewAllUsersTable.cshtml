@model IEnumerable<LuisCorreiaOsteopata.WEB.Models.UserViewModel>

@foreach (var item in Model)
{
    <tr>
        <td>@Html.DisplayFor(modelItem => item.Name)</td>
        <td>@Html.DisplayFor(modelItem => item.Birthdate)</td>
        <td>@Html.DisplayFor(modelItem => item.NIF)</td>
        <td>@Html.DisplayFor(modelItem => item.Email)</td>
        <td>@Html.DisplayFor(modelItem => item.PhoneNumber)</td>
        <td>@Html.DisplayFor(modelItem => item.Role)</td>
        <td>
            <a asp-controller="Account" asp-action="Profile" asp-route-id="@item.Id" class="btn-link">Editar</a>
            |
            <a asp-controller="Appointment" asp-action="ViewAppointments" asp-route-userId="@item.Id" class="btn-link">Ver Consultas</a>
            |
            <a title="Descarregar Ficha" asp-controller="Account" asp-action="DownloadFicha" asp-route-id="@item.Id"><i class="fas fa-download align-content-center me-1"></i></a>
        </td>
    </tr>
}
